<?xml version="1.0" encoding="utf-8" ?>
<DataForm
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://www.enova.pl/schema/form.xsd"
    xsi:schemaLocation="http://www.enova.pl/schema/ http://www.enova.pl/schema/form.xsd"
    Contexts="License.PRJ"
    Priority="10">
  <Page Name="GeneralPage" CaptionHtml="Ogólne" DataContext="{DataSource}">
    <Group CaptionHtml="Ogólne">
        <Row IsReadOnly="true">
            <Field CaptionHtml="Symbol" Width="20" EditValue="{Definition.Symbol}" />
            <Gap Width="*" />
            <Field CaptionHtml="Nazwa" EditValue="{Definition.Name}" Width="50" />
        </Row>
        <Row>
            <Field CaptionHtml="Opis" EditValue="{Description}" Width="80" />
        </Row>
    </Group>
  </Page>
</DataForm>

